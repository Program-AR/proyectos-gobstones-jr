program{ MeterElGol() }

procedure MeterElGol() {
  AvanzarYPatear()
  AvanzarYPatear()
  AvanzarYPatear()
}

procedure AvanzarYPatear() {
  MoverABetoAlEste()
  MoverABetoAlEste()
  PatearLaPelota()
}

procedure PatearLaPelota() {
  MoverElPieDeBeto()
  SacarLaPelota()
  Mover(Este)
  Mover(Este)
  PonerLaPelota()
  Mover(Oeste)
  Mover(Oeste)
}

procedure MoverABetoAlEste() {
  SacarABeto()
  Mover(Este)
  PonerABeto()
}

procedure MoverElPieDeBeto() { Sacar(Azul) Poner(Azul) }

procedure SacarABeto() { Sacar(Azul) Sacar(Azul) }
procedure PonerABeto() { Poner(Azul) Poner(Azul) }

procedure SacarLaPelota() { Sacar(Negro) }
procedure PonerLaPelota() { Poner(Negro) }