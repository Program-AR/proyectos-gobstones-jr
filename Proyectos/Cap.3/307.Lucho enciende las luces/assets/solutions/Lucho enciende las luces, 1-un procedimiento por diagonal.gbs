procedure EncenderLaDiagonalInferior() {
  MoverALuchoAl_(Este)
  MoverALuchoAl_(Este)
  MoverALuchoAl_(Sur)
  EncenderLaLuz()
  MoverALuchoAl_(Sur)
  MoverALuchoAl_(Oeste)
  EncenderLaLuz()
  MoverALuchoAl_(Sur)
  MoverALuchoAl_(Oeste)
  EncenderLaLuz()
  MoverALuchoAl_(Sur)
  MoverALuchoAl_(Oeste)
  EncenderLaLuz()
}

procedure EncenderLaDiagonalSuperior() {
  MoverALuchoAl_(Norte)
  EncenderLaLuz()
  MoverALuchoAl_(Norte)
  MoverALuchoAl_(Este)
  EncenderLaLuz()
  MoverALuchoAl_(Norte)
  MoverALuchoAl_(Este)
  EncenderLaLuz()
  MoverALuchoAl_(Norte)
  MoverALuchoAl_(Este)
  EncenderLaLuz()
}


program {
  EncenderLaDiagonalSuperior()
  EncenderLaDiagonalInferior()
}
