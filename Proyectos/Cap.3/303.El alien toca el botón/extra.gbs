/*@ATTRIBUTE@block_name@Mover al alien al Este@*/
procedure MoverAlAlienAlEste() {
  if (not hayBolitas(Verde)) { BOOM("No estoy sobre el alien") }
  Sacar(Verde) 
  Mover(Este)
  Poner(Verde)
}

procedure TocarElBotón() { 
  if (not hayBolitas(Rojo)) { BOOM("No estoy en el botón") }
  Sacar(Rojo) 
}